###############################################################################
# Logic
###############################################################################

# Implication
<Multi_key> <minus> <greater> : "→" U2192 # implies
<Multi_key> <equal> <greater> : "⇒" U21D2 # fat arrow implies

# Equivalence
<Multi_key> <less> <equal> <greater> : "⇔" U21D4 # material equivalence
<Multi_key> <equal> <equal> : "≡" U2261 # iff
<Multi_key> <less> <minus> <greater> : "↔" U2194 # equivalence

# Operators
<Multi_key> <backslash> <equal> : "¬" U00AC # not
<Multi_key> <slash> <backslash> : "∧" U2227 # and
<Multi_key> <backslash> <slash> : "∨" U2228 # or
<Multi_Key> <backslash> <T> : "⊤" U22A4
<Multi_key> <backslash> <F> : "⊥" U22A5
<Multi_key> <backslash> <A> : "∀" U2200
<Multi_key> <backslash> <E> : "∃" U2203
<Multi_key> <backslash> <plus> : "⊕" U2295 # exclusive disjunction
<Multi_key> <backslash> <bar> <minus> : "⊢" U22A2
<Multi_key> <backslash> <bar> <equal> : "⊨" U22A8
<Multi_key> <backslash> <D> : "◇" U25C7
<Multi_key> <backslash> <B> : "◻" U25A1
<Multi_key> <backslash> <0> : "∅" U2205
<Multi_key> <backslash> <I> : "∈" U2208
<Multi_key> <backslash> <N> : "∉" U2209
<Multi_key> <backslash> <C> : "∘" U2218
<Multi_key> <backslash> <S> : "⊕" U2295
